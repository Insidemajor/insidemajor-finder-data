{
  "name": "college-scorecard-fetcher",  // 패키지 고유 이름
  "version": "2.0.0",                   // 버전 (외부에 배포할 때 의미)
  "description": "Fetches and processes college scorecard data from US Department of Education",  // 설명
  "main": "fetch-api.js",               // 진입점 자바스크립트 파일
  "scripts": {
    "start": "node fetch-api.js",      // npm start 실행 명령어 정의
    "test": "node --check fetch-api.js" // 테스트 명령어(간단히 문법 검사 용도)
  },
  "dependencies": {
    "node-fetch": "^2.7.0"              // HTTP 요청용 의존성 패키지 (버전 2.7 이상 호환)
  },
  "engines": {
    "node": ">=18.0.0"                 // Node.js 실행 환경 최소 버전 명시
  },
  "keywords": ["college", "scorecard", "education", "api"],  // 키워드(검색용)
  "author": "Insidemajor",             // 작성자
  "license": "MIT"                    // 라이선스
}
